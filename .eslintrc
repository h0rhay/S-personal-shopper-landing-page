{
    "extends": [ "wesbos" ],
    "rules": {
        "no-console": 2,
        "no-nested-ternary": 0,
        "react-hooks/exhaustive-deps": 0,
        "react/function-component-definition": 0,
        "prettier/prettier": [
            "error",
            {
            "trailingComma": "es5",
            "singleQuote": true,
            "printWidth": 120,
            "tabWidth": 2,
            }
        ]
    }
}